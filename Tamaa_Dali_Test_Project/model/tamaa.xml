bool tf[AgentNum][TaskNum]={{true,false,false,false},{true,false,false,false},{true,false,false,false},{true,false,false,false}};
bool ts[AgentNum][TaskNum]={{true,false,false,false},{true,false,false,false},{true,false,false,false},{true,false,false,false}};


bool isBusy(int taskID)
{
    int id = 0;
    bool busy = false;

    for(id = 0; id &lt; AgentNum; id++)
    {
        if(ts[id][taskID])
        {
            busy = true;
            return busy;
        }
    }

    return busy;
}

const int rules[MilestoneNum]={0,1,2,3};
const int RegularTaskNum[AgentNum] = {3,3,3,3};
chan move[AgentNum], initialize;

int iteration[AgentNum] = {0,0,0,0};

clock globalTime;

